<van-tabs active="{{ active }}">
  <van-tab title="标签 1">
    <van-notice-bar
      left-icon="volume-o"
      text="在代码阅读过程中人们说脏话的频率是衡量代码质量的唯一标准。"
    />
    <van-panel title="标题" desc="描述信息" use-footer-slot>
      <view class="content">内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容内容</view>
      <view class="footer" slot="footer">
        <van-button size="small">按钮</van-button>
        <van-button style="margin-left:15rpx" size="small" type="danger">按钮</van-button>
      </view>
    </van-panel>
    <van-steps steps="{{ steps }}" active="{{ 2 }}" />
  </van-tab>
  <van-tab title="标签 2">
    <van-card
      num="2"
      tag="标签"
      price="10.00"
      desc="描述信息描述信息描述信息描述信息描述信息描"
      title="这个商品真的很牛逼"
      origin-price="88.00"
      thumb="{{ imageURL }}"
    >
      <view slot="footer">
        <van-button size="mini">按钮</van-button>
        <van-button size="mini">按钮</van-button>
      </view>
    </van-card>
  </van-tab>
  <van-tab title="标签 3">
    <van-submit-bar
      price="{{ 3050 }}"
      button-text="提交订单"
      bind:submit="onClickButton"
      tip="{{ true }}"
    >
      <van-tag type="primary">标签</van-tag>
      <view slot="tip">您的收货地址不支持同城送, <text>修改地址</text></view>
    </van-submit-bar>
  </van-tab>
  <van-tab title="标签 4">
    <van-goods-action>
      <van-goods-action-icon icon="chat-o" text="客服" />
      <van-goods-action-icon icon="cart-o" text="购物车" info="5" />
      <van-goods-action-icon icon="shop-o" text="店铺" />
      <van-goods-action-button color="#be99ff" text="加入购物车" type="warning" />
      <van-goods-action-button color="#7232dd" text="立即购买" />
    </van-goods-action>
  </van-tab>
  <van-tab title="标签 5">
    <van-sidebar active-key="{{ activeKey }}">
      <van-sidebar-item title="标签名" />
      <van-sidebar-item title="标签名" />
      <van-sidebar-item title="标签名" />
  </van-sidebar></van-tab>
  <van-tab title="标签 6">
    <van-skeleton wx:if="{{true}}" row="3" />
    <van-grid>
      <van-grid-item icon="photo-o" text="文字" />
      <van-grid-item icon="photo-o" text="文字" />
      <van-grid-item icon="photo-o" text="文字" />
      <van-grid-item icon="photo-o" text="文字" />
  </van-grid>
  </van-tab>
  <van-tab title="标签 7">
      <van-tree-select
        items="{{ items }}"
        height="55vw"
        main-active-index="{{ mainActiveIndex }}"
        active-id="{{ activeId }}"
        bind:click-nav="onClickNav"
        bind:click-item="onClickItem"
    >
      <image src="https://img.yzcdn.cn/vant/apple-1.jpg" slot="content" />
  </van-tree-select>
  </van-tab>
  <van-tab title="标签 8">
    <van-cell title="选择日期区间"  value="{{ date }}" bindtap="onDisplay" />
    <van-calendar
      show="{{ show }}"
      type="range"
      row-height="50"
      color="#07c160"
      max-date="{{ maxDate }}"
      bind:close="onClose"
      bind:confirm="onConfirm"
    />
  </van-tab>
  <van-tab title="标签 9">
    <van-picker columns="{{ columns }}" bind:change="onChange" />
  </van-tab>
  <van-tab title="标签 10">内容 10</van-tab>
</van-tabs>






